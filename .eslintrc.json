{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "max-len": [
      1,
      120,
      2
    ],
    "no-underscore-dangle": "off",
    "react/static-property-placement": [
      1,
      "static public field"
    ],
    "react/no-array-index-key": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "import/prefer-default-export": "off",
    "object-curly-newline": [
      0
    ],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": [
      0
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/imports-first": [
      "error",
      "absolute-first"
    ]
  }
}
